{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "rcabench",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/experiments_controller/main.go",
      "args": ["both", "-c ./config.dev.toml"],
      "cwd": "${workspaceFolder}/experiments_controller"
    },
    {
      "name": "rcabench sdk dev",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": ["sdk/python/tests"],
      "env": {
        "ENV_MODE": "dev"
      },
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "rcabench sdk prod",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": ["sdk/python/tests"],
      "env": {
        "ENV_MODE": "prod"
      },
      "cwd": "${workspaceFolder}"
    }
  ]
}
