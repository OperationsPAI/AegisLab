{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "rcabench",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src/main.go",
      "args": [
        "both",
        "-c ./config.dev.toml"
      ],
      "cwd": "${workspaceFolder}/src",
      "console": "integratedTerminal",
    },
    {
      "name": "rcabench-producer",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src/main.go",
      "args": [
        "producer",
        "-c ./config.dev.toml"
      ],
      "cwd": "${workspaceFolder}/src",
      "console": "integratedTerminal",
    },
  ]
}